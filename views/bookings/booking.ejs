                    </div>
                    <p class="booked-dates"></p>
                    <input type="date" name="booked[checkIn]" value="<%= checkIn %>" min="<% new Date() %>" class="booked-check-in-date" readonly required>
                    <input type="date" name="booked[checkOut]" value="<%= checkOut %>" min="<% new Date().setDate(new Date().getDate()+1) %>" class="booked-check-out-date" readonly required>
                    <input type="number" name="booked[stayDays]" value="<%= stayDays %>" min="0" max="730" class="booked-staydays" readonly required>
                    <p class="check-in-date-error error mt-2"></p>
                    <p class="check-out-date-error error"></p>
                </div>
                <div class="guests">
                    <div class="guests-top">
                        <h6>Guests</h6>
                        <!-- <span class="edit-guests">Edit</span> -->
                    </div> 
                    <p class="total-guests"></p>
                    <!-- <input type="text" name="booked[guests]" value="<%= Number(guests.adults)+Number(guests.childrens) %>" class="book-total-guests" readonly required> -->
                    <input type="number" name="booked[guests][adults]" value="<%= Number(guests.adults)%>" class="book-total-guests" min="1" max="<%= bookedListing.aboutPlace.guests.adults %>" readonly required>
                    <input type="number" name="booked[guests][childrens]" value="<%= Number(guests.childrens) %>" class="book-total-guests" min="0" max="<%= bookedListing.aboutPlace.guests.adults %>" readonly required>
                    <input type="number" name="booked[infants]" value="<%= Number(infants)%>" class="book-total-infants" min="0" max="<%= bookedListing.aboutPlace.guests.infants %>" readonly required>
                    <input type="number" name="booked[pets]" value="<%= Number(pets) %>" class="book-total-pets" min="0" max="<%= bookedListing.aboutPlace.guests.pets %>" readonly required>
                    <p class="guests-error error mt-2"></p>
                    <p class="infants-error error"></p>
                    <p class="pets-error error"></p>
                </div>
        </div>
        <!-- <div class="request request-sec2 payment error-hide hide">
            <h4>Pay with</h4>
            <div class="opt"></div>
            <div class="opt"></div>
        </div> -->
        <div class="request request-sec3 error-hide">
            <h4>Required for your trip</h4>
            <div class="require-message">
                <div class="add-msg">                    
                    <h6>Message the host</h6>
                    <button class="btn btn-outline-dark btn-sm" type="button">Add</button>
                </div>
                <p class="msg-host"></p>
            </div>
            <div class="require-no">
                <div class="add-no">
                    <h6>Phone number</h6>
                    <button class="btn btn-outline-dark btn-sm" type="button">Add</button>
                </div>
                <p>Add and confirm your phone number to get trip updates.</p>
            </div>
        </div>
        <div class="request request-sec4 error-hide">
            <h4>Cancellation policy</h4>
            <p class="booked-cancellation-policy"></p>
        </div>
        <div class="request request-sec5 error-hide">
            <h4>Ground rules</h4>
            <p class="mb-3">We ask every guest to remember a few simple things about what makes a great guest.</p>
            <p class="mb-2">· &nbsp; Follow the house rules</p>
            <p class="mt-1">· &nbsp; Treat your Host’s home like your own</p>
        </div>
        <div class="request request-sec6 error-hide">
           <i class="fa-solid fa-calendar-check"></i>
           <p><b>The Host will need to accept this request.</b> You’ll pay now, but will get a full refund if your reservation isn’t confirmed within 24 hours.</p>
        </div>

        <button class="book-btn error-hide submitBtn" onclick="makePayment()" type="button">
            <span class="submitBtn-text">Request to book</span>
            <div class="spinner" style="display: none;">
                <span class="dot dot-1"></span>
                <span class="dot dot-2"></span>
                <span class="dot dot-3"></span>
            </div>
        </button>  
    </div>

    <div class="booking-card">
        <div class="card-top">
            <div class="card-img">
                <img src="<%= bookedListing.images[0].path %>" alt="listing-img">
            </div>
           <div class="card-details">
                <h6 class="listing-title"><%= bookedListing.title %></h6>
                <p class="listing-type"><%= bookedListing.privacyType %> <%= bookedListing.houseType %></p>
