<script src="https://checkout.razorpay.com/v1/checkout.js"></script>
<script>  
    document.title="Confirm and pay | Airbnb";
    let keyId = '<%= process.env.RZR_KEY_ID %>';  
    let id = '<%= id %>';  
    let bookListing = <%- JSON.stringify(bookedListing) %>;    
    // let bookListing = JSON.parse('<%- JSON.stringify(bookedListing) %>'); 
    let currGuest = JSON.parse('<%- JSON.stringify(currUser) %>');    
    let days = JSON.parse('<%- JSON.stringify(stayDays) %>');   
    let isSuccess = "";
    let defaultImg = "https://images.unsplash.com/photo-1749253851355-3529792f0d34?w=500&auto=format&fit=crop&q=60&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHx0b3BpYy1mZWVkfDQ2fHRvd0paRnNrcEdnfHxlbnwwfHx8fHw%3D";

    async function makePayment() {

        const amount = document.getElementById('booked-amount').value;  

        if (amount === '' || amount <= 0) {
            alert('Please enter a valid amount');
            return;
        };
        
        const response = await fetch(`/listings/${id}/book/create-order`, {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ amount })
        })  
        const order = await response.json();
                    
        const options = {
            key: keyId, 
            amount : amount, 
            currency : currGuest.countryInfo ? currGuest.countryInfo.currency : "INR",
            name: 'Airbnb',
            image: currGuest.profileImg ? currGuest.profileImg : defaultImg,
            description: bookListing.title,
            order_id: order.id,
            // callback_url: `/listings/${id}`, // Your success URL
            handler: async function (response) {
                // Verify payment here
                let res = await fetch(`/listings/${id}/book/verify-payment`, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    razorpay_order_id: response.razorpay_order_id,
                    razorpay_payment_id: response.razorpay_payment_id,
                    razorpay_signature: response.razorpay_signature
                })
                })
                let result = await res.json();
                isSuccess = result.status;  
                // alert (result.message);
                submitForm();
            },
            prefill: {
                name: currGuest.username,
                email: currGuest.email,
                contact: currGuest.phoneNo || "0000000000", // mhuje user ka no bhi add krwana hoga
            },
            theme: {
            color: '#FF385C'
            },
        };
            
        const rzp1 = new Razorpay(options);
        rzp1.open();
    }
    
    function submitForm() {     
        if (isSuccess == "success") {
            document.querySelector("#booking-form").submit();
        }
    };    
    
    let editDates = document.querySelector(".edit-dates"); 
    let editGuests = document.querySelector(".edit-guests");

    let formatDate = (date)=>{
        let y = date.getFullYear();
        let m = String(date.getMonth() + 1).padStart(2, "0");
        let d = String(date.getDate()).padStart(2, "0");
        return `${y}-${m}-${d}`;
    };

    function displayDates() {
        let bookedDates = document.querySelector(".booked-dates");
        // let bookedCheckInDates = document.querySelector(".booked-check-in-date");
        // let bookedCheckOutDates = document.querySelector(".booked-check-out-date");

        let checkInFrom = new Date('<%- JSON.stringify(checkIn) %>');
        let checkOutTo = new Date('<%- JSON.stringify(checkOut) %>');        

        if(checkInFrom.getFullYear() == checkOutTo.getFullYear() && checkInFrom.getMonth() == checkOutTo.getMonth()){
            bookedDates.innerHTML = `${checkInFrom.toDateString().split(" ").reverse().slice(1,2).join(" ")} - ${checkOutTo.toDateString().split(" ").reverse().slice(1,3).join(" ")}`;
        }else if(checkInFrom.getFullYear() == checkOutTo.getFullYear() && checkInFrom.getMonth() != checkOutTo.getMonth()){            
            bookedDates.innerHTML = `${checkInFrom.toDateString().split(" ").reverse().slice(1,3).join(" ")} - ${checkOutTo.toDateString().split(" ").reverse().slice(1,3).join(" ")}`;
        } else{
            bookedDates.innerHTML = `${checkInFrom.toDateString().split(" ").slice(1,4).join(" ")} - ${checkOutTo.toDateString().split(" ").slice(1,4).join(" ")}`;
        }

        // bookedCheckInDates.value = `${formatDate(checkInFrom)}`;
        // bookedCheckOutDates.value = `${formatDate(checkOutTo)}`;
        
    }
    displayDates();

    function displayGuests() {        
        let totalGuests = document.querySelector(".total-guests");
        let bookTotalGuests = document.querySelector(".book-total-guests");

        let guestsError = document.querySelector(".guests-error");
        let infantsError = document.querySelector(".infants-error");
        let petsError = document.querySelector(".pets-error");
        let checkInError = document.querySelector(".check-in-date-error");
        let checkOutError = document.querySelector(".check-out-date-error");
        
        let bookedStaydays = document.querySelector(".booked-staydays");
        
        let cardBottomError = document.querySelector(".card-error");
        let cardErrorText = document.querySelector(".card-error-text");
        let errorHide = document.querySelectorAll(".error-hide");

        let g = JSON.parse('<%- JSON.stringify(guests) %>')
        let infants = Number(JSON.parse('<%- JSON.stringify(infants) %>'));
        let pets = Number(JSON.parse('<%- JSON.stringify(pets) %>'));
        let allGuests = Number(g.adults) + Number(g.childrens);

        let chkIn = JSON.parse('<%- JSON.stringify(checkIn) %>');
        let chkOut = JSON.parse('<%- JSON.stringify(checkOut) %>');
        let stayDays = JSON.parse('<%- JSON.stringify(stayDays) %>');

        // let d = new Date(chkIn);
        // let dayCount = 0;
        // for (let i = 1; d < new Date(chkOut); i++) {
        //     d.setDate(d.getDate()+1);
        //     dayCount = i;
        // };
        
        // days = dayCount;
        // bookedStaydays.value = dayCount;

        let infantStr = infants > 1 ? `, ${infants} infants` : infants == 1 ? `, ${infants} infant` : '';
        let petStr = pets > 1 ? `, ${pets} pets` : pets == 1 ? `, ${pets} pet` : '';
        let guestStr = allGuests > 1 ? `${allGuests} guests` : allGuests == 1 ? `${allGuests} guest` : '';        
        
        totalGuests.innerHTML = `${guestStr}${infantStr}${petStr}`;
        // bookTotalGuests.innerHTML = `${guestStr}${infantStr}${petStr}`;

        // Error texts and card error

        let guestErr = "";
        let infantErr ="";
        let petErr ="";
        let checkInErr ="";
        let checkOutErr ="";

        if(allGuests <= 0 ){
            guestErr = "Guest number should be valid";
        }else if(Number(g.adults) <= 0 ){
            guestErr = "Adult should be valid (minimum 1 adult)";
        }else if (allGuests > bookListing.aboutPlace.guests.adults) {
            guestErr = "Reduce the guest count to continue";
        }else{};
        
        if (infants > bookListing.aboutPlace.guests.infants || infants < 0) {
            if(bookListing.aboutPlace.guests.infants == 0){               
                infantErr = "This place does not allow infants";
            }else if(infants < 0){               
                infantErr = "Infants number should be valid";
            }else { 
                infantErr = "Reduce the infants count to continue";
            }           
        }

        if (pets > bookListing.aboutPlace.guests.pets || pets < 0) {           
            if(bookListing.aboutPlace.guests.pets == 0){
                petErr = "This place does not allow pets";
            }else if(pets < 0){
                petErr = "Pets number should be valid";
            } else {
                petErr = "Reduce the pets count to continue";
            }
        }

        // infantErr = infants > bookListing.aboutPlace.guests.infants ? bookListing.aboutPlace.guests.infants ==0 ? `This place does not allow infants.` : `Reduce the guest count to continue` : "";
        // let petErr = pets > bookListing.aboutPlace.guests.pets ? bookListing.aboutPlace.guests.pets==0 ? `This place does not allow pets.` : `Reduce the pets count to continue` : "";
             
        if (chkIn >= chkOut) {
            checkInErr = "Check in check out dates should be valid";
        }else if(chkIn < formatDate(new Date())){
            checkInErr = "Check in dates should be valid";
        }else{};

        if(chkOut <= formatDate(new Date())){
            checkOutErr = "Check out dates should be valid";
        }  
        

        guestsError.innerHTML = guestErr;
        infantsError.innerHTML = infantErr;
        petsError.innerHTML = petErr;
        checkInError.innerHTML = checkInErr;
        checkOutError.innerHTML = checkOutErr;

        if(guestErr || infantErr || petErr || checkInErr || checkOutErr){
            if( guestErr || infantErr ){
                cardErrorText.innerHTML = `This place only allows up to ${bookListing.aboutPlace.guests.adults} guests.`;            
            }else if(petErr){
                if(bookListing.aboutPlace.guests.pets==0){
                    cardErrorText.innerHTML = `This place does not allow pets.`;
                } else{
                    cardErrorText.innerHTML = `This place only allows up to ${bookListing.aboutPlace.guests.pets} pets.`;
                }                
            }else if(checkInErr || checkOutErr){
                cardErrorText.innerHTML =  "Check in check out dates should be valid";
            }else {}
           
            cardBottomError.hidden = false;
            errorHide.forEach((el)=>{
                el.hidden = true;
            });
        }
        // else{
        //     errorHide.forEach((el)=>{
        //         el.hidden = false;
        //     });
        // }
    }
    displayGuests();

    function cancellationPolicy() {
        let bookedCancellationPolicy = document.querySelector(".booked-cancellation-policy");
        let checkInDay = new Date('<%- JSON.stringify(checkIn) %>');
        let checkOutDay = new Date('<%- JSON.stringify(checkOut) %>');
        let checkInBeforeDay = new Date(checkInDay);        
        checkInBeforeDay.setDate(checkInDay.getDate()-1);
        let checkInBeforeDayMonth = checkInBeforeDay.toDateString().split(" ").slice(1,2);
        let checkInDayMonth = checkInDay.toDateString().split(" ").slice(1,2);

        if((checkOutDay.getDate() - checkInDay.getDate() == 1 && checkInDay.getMonth() == checkOutDay.getMonth()) || checkInDay.getMonth() != checkOutDay.getMonth() && (checkOutDay.getDate() != checkInDay.getDate() && (!days > 1 || days ==1))){       
            bookedCancellationPolicy.innerHTML = `<b> Free cancellation before ${checkInDay.getDate()} ${checkInDayMonth} </b>. After that, the reservation is non-refundable.`;
        } else {   
            if(checkInBeforeDay.getDate() == new Date().getDate() && checkInBeforeDay.getMonth() == new Date().getMonth()){
                bookedCancellationPolicy.innerHTML = `<b>Free cancellation today.</b> After that, the reservation is non-refundable.`;
            }else {
                bookedCancellationPolicy.innerHTML = `<b>Free cancellation before ${checkInBeforeDay.getDate()} ${checkInBeforeDayMonth}.</b> Cancel before check-in on ${checkInDay.getDate()} ${checkInDayMonth} for a partial refund.`;
            }
        }  
    }
    cancellationPolicy(); 

    function displayAmounts() {

        let nightsPrice = document.querySelector(".nights-price");
        let nightsDates = document.querySelector(".night-dates");
        let servicePrice = document.querySelector(".service-amount");
        let totalPrice = document.querySelector(".total-price");
        let totalPriceBreakdown = document.querySelector(".total-price-breakdown");
        let taxAmounts = document.querySelectorAll(".tax-amount");
        let totalAmounts = document.querySelectorAll(".total-amount");
        let bookedTotalAmounts = document.querySelector(".booked-total-amount");
        let priceCurrency = document.querySelectorAll(".price-currency");
        let priceBreakdown = document.querySelector(".price-breakdown");
        let closePriceBreakdown = document.querySelector(".price-breakdown-container .cross");
        let priceBreakdownContainer = document.querySelector(".price-breakdown-container");
        
        let bookedDates = document.querySelector(".booked-dates");
        
        let msgHost = document.querySelector(".request-sec3 .msg-host");
        let listingReviewAndRating = document.querySelector(".card-top .listing-ratings");       
       
        msgHost.innerHTML = `Before you can continue, let ${bookListing.owner.username} know a little about your trip and why their place is a good fit.`;
        
        listingReviewAndRating.innerHTML = bookListing.reviews.length > 0 ?
        `<i class="fa-solid fa-star"></i> <span class="avg-rating">${'<%= avgRating %>'}</span> &nbsp; (${Number(bookListing.reviews.length).toLocaleString(navigator.language || "en-IN")} ${bookListing.reviews.length > 1 ? "reviews" : "review"})` : "<small>No reviews</small>"; 

        priceCurrency.forEach((priceCurr)=>{
            priceCurr.innerHTML = `(${currGuest.countryInfo ? currGuest.countryInfo.currency || "INR" : "INR"})`;
        })

        // card price * nights
        if(days>27){
            nightsPrice.innerHTML = `Average monthly price`;
        } else{       
            nightsPrice.innerHTML = `<%= convertedObj.listingPrice %> &nbsp;<i class="fa-solid fa-x"></i>&nbsp; ${days > 1 ? days+" nights" : days+" night"}`;
        }

        // price-break-down div's days & dates
        nightsDates.innerHTML = `${days > 1 ? days+" nights" : days+" night"} · ${bookedDates.innerHTML}`;
        
        // price-break-down div's price        
        totalPriceBreakdown.innerHTML = '<%= convertedObj.price %>';      

        // price-break-down div's service       
        servicePrice.innerHTML = '<%= convertedObj.svc %>';  

        // card-bottom div's price + service       
        totalPrice.innerHTML = '<%= convertedObj.priceAndSvc %>';    

        // card-bottom div's tax        
        taxAmounts.forEach((taxAmount)=>{
            taxAmount.innerHTML = '<%= convertedObj.tax %>';
        });

        // card-bottom div's total amount       
        totalAmounts.forEach((totalAmount)=>{
            totalAmount.innerHTML = '<%= convertedObj.finalAmount %>';  
        });

        bookedTotalAmounts.value = '<%= convertedObj.finalAmount.replace(/[^\d.]/g, "") %>'; 

        // bookedTotalAmounts.value = `${finalAmount.toLocaleString(
        //     navigator.language || "en-IN", 
        //     {style: "currency", currency : currGuest.countryInfo ? currGuest.countryInfo.currency || "INR" : "INR",}
        // )}`;           

        // price-break-down div open & close
        priceBreakdown.addEventListener("click", (e)=>{
            e.stopPropagation()
            priceBreakdownContainer.hidden = false;
        });

        closePriceBreakdown.addEventListener("click", (e)=>{
            e.stopPropagation()
            priceBreakdownContainer.hidden = true;
        });
        // popup p clik krne p bhi y hidden ho ra h
        document.addEventListener("click",(e)=>{
            let p = document.querySelector(".card-bottom");
            if(!p.contains(e.target)){
                priceBreakdownContainer.hidden = true;
            }
        })

    }
    displayAmounts();
    


