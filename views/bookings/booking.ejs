<% layout("./layouts/boilerplate.ejs") %>

<style>
    #nav-search-box, input{
        display: none;
    }
    #nav-div{
        height: fit-content;
    }
    .listing-booking-page{
        padding: 3.5rem 14vw;
        display: flex;
        gap: 90px;
    }  
    
    .listing-booking-page h4{
        font-weight: 600;
        font-size: 1.37rem;
        margin-bottom: 0;
        margin-bottom: 1.5rem;
    }

    .book-request{
        width: fit-content;
        max-width: 40rem;
        padding: 0 1.65rem;
    }
  
    .book-request .request-heading{
        display: flex;
        align-items: center;
        color: #222222;
        gap: 18px;
        margin-bottom: 1rem;
    }
    .book-request .request-heading a{
        color: #222222;
        margin-left: -30px;
    }
    .book-request .request-heading h2{
        font-weight: 600;
    }
    .book-request .request{ 
        padding: 2rem 0;
        border-bottom: 1px solid gainsboro;
    }
    .book-request .request p{
        color: #141414;
        margin-bottom: 0;
    }
    .book-request .request-sec1 .dates, .book-request .request-sec1 .guests{
        margin-top: 1.35rem;
    }
    .book-request .request-sec1 .dates-top, .book-request .request-sec1 .guests-top{
        display: flex;
        align-items: center;
        justify-content: space-between;
    }
    .book-request .request-sec1 .dates h6, .book-request .request-sec1 .guests h6{
        font-weight: 600;
    }
    .book-request .request-sec1 .dates span, .book-request .request-sec1 .guests span{
        font-weight: 600;
        text-decoration: underline;
        cursor: pointer;
    }
    .book-request .request-sec1 .dates p, .book-request .request-sec1 .guests p{
       
    }

    /* .book-request .request-sec2 .opt{
        padding: 1.5rem 0;
        border-radius: 12px;
        border: 1px solid black;
        margin-top: 1rem;
    } */

    .book-request .request-sec3 .require-no{
        margin-top: 1.5rem;
    }
    .book-request .request-sec3 .add-msg,  .book-request .request-sec3 .add-no{
        display: flex;
        align-items: center;
        justify-content: space-between;
    }
    .book-request .request-sec3 h6{
        font-weight: 600;
        margin-bottom: -.25rem;
    }
    .book-request .request-sec3 p{
        line-height: 1.27;
        font-size: .87rem;
        width: 80%;
    }
    .book-request .request-sec3 button{
        font-weight: 600;
        padding: .35rem .85rem;
        border-radius: 8px;
    }
    .book-request .request-sec4 p, .request-sec5 p, .request-sec6 p{  
        font-size: .97rem;              
        line-height: 1.27;
    } 

    .book-request .request-sec6{
        display: flex;
        gap: 25px;
    }
    .book-request .request-sec6 i{
        margin-top: .25rem;
        font-size: 1.65rem;
    }

    .book-request button.book-btn{
        margin-top: 3.5rem;
        padding: 1rem 2.25rem;
        font-size: 1.05rem;
        font-weight: 600;
        border-radius: 8px;  
        border: none;
        outline: none;  
        color: #fff;
        background: linear-gradient(to right, #f33155,  rgb(192, 30, 92));
    }

    .booking-card{
        width: 29rem;
        min-width: 28rem;
        height: fit-content;
        position: sticky;
        top: 8.5rem;
        border: 1px solid #dbdbdb;
        border-radius: 12px;
        padding: 1.5rem;
        margin-top: 5rem;
        color: #0e0e0e;
    }
    .booking-card p{
        margin-bottom: 0;
    }
    .booking-card .card-top{
        display: flex;
        align-items: center;
        gap: 15px;
        padding-bottom: 1.5rem;
        border-bottom: 1px solid #e7e7e7;
    }
    .booking-card .card-top .card-img{
        height: 6.25rem;
        width: 6.5rem;
        border-radius: 12px;
        overflow: hidden;
    }
    .booking-card .card-top img{
        height: 100%;
        width: 100%;
        object-fit: cover;
        object-position: center;
    }
    .booking-card .card-details i{
        font-size: .68rem;
    } 
    .booking-card .card-top h6{
        font-weight: 600;
    }
    .booking-card .card-top p{
        font-size: .85rem;
    }
    .booking-card .card-top p span{
        font-weight: 600;
    }

    .error{
        font-size: .83rem;
        font-weight: 500;
        line-height: 1.25;
        color: #b62424 !important;
    }

    .card-bottom{
        padding-top: 1.5rem;
    }
    .card-error{
        padding: 3rem 0 1.65rem;
        display: flex;
        align-items: center;
        justify-content: center;
    }
    .card-error P{        
        font-size: .9rem;
        color: #808080 !important;
    }
    .card-bottom .avg-price i{
        font-size: .63rem;
    }
    .card-bottom .avg-price, .taxes, .total{
        display: flex;
        align-items: center;
        justify-content: space-between;
        margin-bottom: .65rem;
    }
    .card-bottom h6{
        font-weight: 600;
        color: #1f1f1f;
        font-size: .85rem;
        line-height: 1;
    }
    .card-bottom p{
    }
    .card-bottom .total{
        font-weight: 600;
        padding-top: 1.25rem;
        margin-top: 1.25rem;
        border-top: 1px solid #d1d1d1;
    }
    .card-bottom span{
        color: #020202;
    }
    .price-breakdown{
        text-decoration: underline;
        font-weight: 600;
        cursor: pointer;        
        width: fit-content;
        float: right;
    }

    .popup-container{
        position: fixed;
        top: 0;
        left: 0;
        height: 100vh;
        width: 100vw;        
        z-index: 5;        
        background-color: #20202065;
    }
   
    .popup-div{
        position: fixed;
        overflow-y: scroll;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
        max-height: 90vh;   
        height: auto;     
        width: 33rem;
        padding-top: 0;
        border-radius: 20px;
        background-color: #fff;
        animation: translateY .65s cubic-bezier(0.19, 1, 0.22, 1) 1;  
        box-shadow: 1px 1px 25px rgba(0, 0, 0, 0.150),
        -1px -1px 25px rgba(0, 0, 0, 0.150);
    }
    .popup-div::-webkit-scrollbar{
        display: none;
    }
    .popup-div .body{  
        padding-top: 0;      
        padding: 0 25px 15px;
    }

    .popup-div .body p{
        padding: .15rem 0;
        margin: 0;
        color: #1f1f1f;
    }

    .popup-div .header{
        background-color: #ffffff;
        height: 4.75rem;
        position: sticky;
        top: 0;
        left: 0;
        display: flex;
        padding: 0 25px;
        align-items: center;
        position: relative;
        border-bottom: .5px solid rgba(206, 205, 205, 0.918);
    }
    .popup-div .header span{
        font-weight: 600;
        position: absolute;
        left: 50%;
        top: 50%;
        transform: translate(-50%,-50%);
    }
    .popup-div .cross{         
        height: 32px;
        width: 32px;
        display: flex;
        align-items: center;
        justify-content: center;
        transform: translate(-28%,0);
        border-radius: 50%;
        font-size: 1.05rem;
        cursor: pointer;
        color: rgb(58, 58, 58);
        transition: all cubic-bezier(0.19, 1, 0.22, 1) .25s; 
    }    
    .popup-div .cross:hover{
        background-color: rgb(245, 253, 255);
    }

    @media screen and (max-width : 1350px) {
        .listing-booking-page{
            padding: 3.5rem 10vw;
            flex-wrap: wrap;
            justify-items: center;
            /* flex-direction: column-reverse; */
            gap: 50px;
        } 
        .book-request{
            width: 100%;
            max-width: 100%;
        }
        .booking-card{
            position: static;
            margin-top: 1.5rem;
        }
    }

    @media screen and (max-width : 850px) {
        .listing-booking-page{
            padding: 1.75rem 3.5vw;
        } 
       
        .booking-card{
            width: 100%;
            min-width: 100%;
        }
    }
    @media screen and (max-width : 650px) {       
        .book-request{
            padding: 0;
        }
        .book-request .request-heading a{
            margin-left: 0px;
        }
        .book-request .request-heading{
            margin-bottom: 0;
        }
        .popup-div{
            width: 95%;
        }        
    }

    @media screen and (max-width : 400px) {
        .booking-card{
            padding: .85rem;
        }
    }
    
</style>

<body>

    <div class="listing-booking-page">

